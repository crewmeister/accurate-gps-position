{
  "name": "accurate-gps-position",
  "version": "2.0.0",
  "description": "Accurate GPS position retreival",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register src/*spec.js src/**/*spec.js --bail --reporter=dot --slow=1",
    "watch:test": "npm run test -- --watch",
    "start": "npm run build:watch",
    "_start:js": "mkdir -p dist; watchify --transform=babelify --outfile=dist/index.js src/index.js --verbose",
    "_start:html": "chokidar --initial 'src/index.html' -c 'npm run _build:html'",
    "build": "mkdir -p ./dist; cp ./example/index.html dist/; cp src/index.js dist/",
    "postbuild": "uglifyjs dist/index.js -m -c > dist/index.min.js",
    "watch:build": "nodemon --watch src --watch example --ext js,html,css --exec npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crewmeister/accurateGpsPosition.git"
  },
  "keywords": [
    "gps"
  ],
  "author": "Greg Wilson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crewmeister/accurateGpsPosition/issues"
  },
  "homepage": "https://github.com/crewmeister/accurateGpsPosition#readme",
  "devDependencies": {
    "nodemon": "^1.9.2",
    "uglify-js": "^2.6.2",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "mocha": "^2.4.5",
    "watchify": "^3.7.0"
  }
}
